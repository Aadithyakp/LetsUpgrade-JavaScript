        add = numOne+numTwo;
        sub = numOne-numTwo;
        div = numOne/numTwo;
        mul = numOne*numTwo;
        modulus = numOne%numTwo;
        if(numOne!=numTwo)
        {
        if(numOne>numTwo)
        console.log(numOne+ " is the largest")
        else
        console.log(numTwo+ " is the largest")
        }
        else{
            console.log("Both numbers are equal")
        }
        console.log("Addition = "+add)
        console.log("Substraction = "+sub)
        console.log("Division = "+div)
        console.log("Multiplication = "+mul)
        console.log("Remainder = "+modulus)
